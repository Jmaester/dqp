<body><script type="text/javascript">
    function openFriendLink(friendLink) {
      window.open(friendLink,
        "invitefriend",
        "width=700,height=650,location=no,menubar=no,"+
        "status=no,titlebar=no,toolbar=no,scrollbars=yes,resizable=yes");
    }
    function openLink(anchor) {
      window.open(anchor.href, anchor.target,
        "width=700,height=650,toolbar,titlebar,scrollbars,resizable").focus();
      return false;
    }
    function submitForm() {
      var hlvalue = document.getElementById("hl").value;
      href = new String(location.href);
      if (href.indexOf("hl=") != -1) {
        var replacement = new String("hl=" + hlvalue);
        href = href.replace(/hl=\w+/, replacement);
      } else if ( href.indexOf("?") == -1) {
        href = href + "?hl=" + hlvalue;
      } else {
        href = href + "&hl=" + hlvalue;
      }
      location.href = href;
      return true;
    }
  </script>
