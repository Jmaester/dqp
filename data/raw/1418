          var showLangSelector = false;
          
            var agent = navigator.userAgent.toLowerCase();
            var isIe = (agent.indexOf('msie') != -1);
            if (showLangSelector && isIe) {
              document.getElementById('logo_image').style.margin = "1.2em 0.8em 0 0";
            } else if (showLangSelector && !isIe) {
              document.getElementById('logo_image').style.margin = "1.5em 0.8em 0 0";
