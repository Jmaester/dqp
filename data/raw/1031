<script id="form-validate-js" type="text/javascript">
function formValidation_abuse(form,target) {
init(form,target);
var temp1;
var temp2;
var field = form.elements["extra.redirect_abuse"];
for (var i = 0; i < field.length; i++) {
if (field[i].checked) {
document.location = field[i].value;
}
}
return false;
var validated = checkErrors(form,target);
return validated;
}
</script>
