<script type="text/javascript">
        // Inject event handlers
        document.getElementById("fullName").onchange = updateImprintName;
        document.getElementById("companyName").onchange = updateImprintName;
        var c_cs = document.getElementById("countrySelector");
        c_cs.onchange = updateCountry;
        _AF_AddObserver("contactInput.asciiAddressInput", function (id) {
          var country = _AF_GetCountry(id);
          if (country != c_cs.value) {
            c_cs.value = country;
          }
        });

        // Initialize the form after the HTML is loaded.
        displayMetadataForContentType();
        updateCountry();
        displayAddressForAccountType();
      </script></body></html>