      function _AF_ToggleChildren(node) {
        var list = node.parentNode.childNodes;
        for (var i=0; i < list.length; i++) {
          var item = list.item(i);
          if (item.nodeType == 1) {
            item.style.display = (node == item) ? "none" : "";
          }
        }
      }
    </script>
