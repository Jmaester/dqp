<script type="text/javascript">

    function updateCountry() {
      var country = document.getElementById("countrySelector").value;
      if (_AF_GetCountry("contactInput.asciiAddressInput") != country) {
        _AF_SetCountry("contactInput.asciiAddressInput", country, true);
      }

      switch(country) {
        case "TW":
        case "HK":
        case "JP":
        case "CN":
        case "MO":
        case "KR":
        case "RU":
          document.getElementById("languageNotes").style.display = 'block';
          break;
        default:
          document.getElementById("languageNotes").style.display = 'none';
      }

      language = "en"
      if (language == "ja" || language == 'ko' || language == 'zh' || language == 'ru') {
        document.getElementById("languageNotes").style.display = 'block';
      }
    }

    // Return the currently radio button value
    function getSelectedValue(name) {
      var rbs = document.getElementsByName(name);
      for (var i = 0; i < rbs.length; i++) {
        if (rbs[i].checked) {
          return rbs[i].value;
        }
      }
    }

    // Change the payee name fields depending on the selection
    // of "Individual" or "Organization" (aka Business).
    //
    function displayAddressForAccountType() {
      var opt = getSelectedValue("publisherInformation.accountType");

      if (opt == "Organization") {
        document.getElementById("companyNameRowId").style.display = '';
        document.getElementById("languageNoteCompany").style.display = 'block';
        document.getElementById("languageNotePerson").style.display = 'none';
      }
      else {  // opt == "Individual"
        document.getElementById("companyNameRowId").style.display = 'none';
        document.getElementById("companyName").value = "";
        document.getElementById("languageNoteCompany").style.display = 'none';
        document.getElementById("languageNotePerson").style.display = 'block';
      }

      updateImprintName();
    }

    var imprintNameTouched = false;

    // Assign first 28 characters of companyName field as a 'ImprintName'.
    function updateImprintName() {
      if (imprintNameTouched) return;

      var imprintName = document.getElementById("imprintName");
      var imprintNameValue;

      if (document.getElementById("companyNameRowId").style.display == '') {
        imprintNameValue = document.getElementById("companyName").value;
      } else {
        imprintNameValue = document.getElementById("fullName").value;
      }

      imprintName.value = imprintNameValue.substring(0, 27);
    }

    // Show or don't show the Magazine serial fields depending on whether
    // the magazine content type checkbox is selected.
    //
    function displayMetadataForContentType() {
    
    }

  </script>
